#     - name: "z-ai/glm-5:free"
#       alias: "glm-5"
# oauth-excluded-models:
#   kilo:
#     - "kilo-claude-opus-4-6"  # exclude specific models (exact match)
#     - "*:free"                # wildcard matching suffix (e.g. all free models)

