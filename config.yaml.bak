# CLIProxyAPI Configuration
# CLI Pool Proxy for Round-Robin rotation across 6 Google Accounts

# Server settings
host: "0.0.0.0"
port: 8317
# TLS settings (disabled for local/internal use)
tls:
  enable: false
# Management API (enabled for Web UI)
remote-management:
  allow-remote: true
  secret-key: "$2a$10$hmngFSN7p4dVVCIH/OsOsufH4aProYYpeqB2pe9FyR4C2W0m4mcWK"
  disable-control-panel: false
  panel-github-repository: https://github.com/router-for-me/Cli-Proxy-API-Management-Center
# Authentication directory (mounted in Docker)
auth-dir: "/root/.cli-proxy-api"
# API Key - same as Antigravity Manager
api-keys: []
# Debug logging (disabled to avoid buffer issues with large payloads)
debug: true
# Logging configuration
logging-to-file: true
logs-max-total-size-mb: 100
error-logs-max-files: 10
# Usage statistics
usage-statistics-enabled: true
# Request settings
request-retry: 3
max-retry-interval: 30
# Quota handling - switch to next account when exhausted
quota-exceeded:
  switch-project: true
  switch-preview-model: true
# Routing strategy - round-robin for even distribution
routing:
  strategy: "fill-first"
# WebSocket auth disabled
ws-auth: false
# Keep-alive to prevent context cancellation on long requests (renew every 30 seconds)
nonstream-keepalive-interval: 30
# OAuth model aliases for Gemini CLI - DEFINE ALL MODELS
oauth-model-alias: {}
# Request logging (disabled to avoid buffer issues)
request-log: false
request-logging:
  enable: false
# Gemini CLI OAuth configuration
gemini-api-key: []
# OpenAI compatibility providers
openai-compatibility: []
